{
	"name": "mmt19_admin",
	"version": "0.1.0",
	"description": "Secure admin SPA for managing Bookings, Tickets and users as well as statistics for OTD of the event",
	"main": "client/index.js",
	"author": "Normanhurst Rover Crew",
	"license": "ISC",
	"scripts": {
		"test-only": "mocha --require @babel/register --require ignore-styles --recursive tests/",
		"test": "nyc --reporter=html npm run test-only",
		"check-coverage": "nyc check-coverage --lines 100",
		"build": "webpack --mode production --colors && babel server/server.js -d dist",
		"start": "webpack-dev-server --hot --host 0.0.0.0 --mode development",
		"prod": "node dist/server.js",
		"dev": "webpack-dev-server --hot --host 0.0.0.0 --mode development & nodemon --exec babel-node -L server/server.js 0.0.0.0 3000 -w ./server"
	},
	"dependencies": {
