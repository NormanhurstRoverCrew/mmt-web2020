{
	"version": "0.1.0",
	"configurations": [		
		{
			"name": "Launch admin$localhost",
			"type": "chrome",
			"request": "launch",
			"url": "http://localhost:8081/",
			"webRoot": "${workspaceFolder}/admin/"
		},
		{
			"name": "Launch rego$localhost",
			"type": "chrome",
			"request": "launch",
			"url": "http://localhost:8080/",
			"webRoot": "${workspaceFolder}/rego/"
		},
		{
			"name": "Launch index.html",
			"type": "chrome",
			"request": "launch",
			"file": "${workspaceFolder}/index.html"
		},
	]
}